{
  "name": "punahou_canvas",
  "version": "1.0.0",
  "description": "r/place for Punahou",
  "main": "index.js",
  "scripts": {
    "install": "npm-run-all install-frontend install-backend",
    "install-frontend": "cd ui && npm install",
    "install-backend": "cd api && npm install",
    "dev": "npm-run-all --parallel dev-frontend dev-backend",
    "dev-frontend": "cd ui && npm run dev",
    "dev-backend": "cd api && npm run dev",
    "start": "npm-run-all --parallel ui api"
  },
  "author": "Alyx, MisterKermit",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
